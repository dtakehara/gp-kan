{
    "BASELINE_JITTER": 1e-2,
    "GLOBAL_JITTER": 1e-2,
    "EPOCHS": 100,
    "BATCH_SIZE": 64,
    "LEARNING_RATE": 1e-2,
    "MODEL":[
        { "GP_conv2D": {
            "IH_IW_IC_OC": [28, 28, 1, 1],
            "kernel_size": 4, "stride": 2, "num_gp_pts": 10,"use_double_layer": true
        }},
        { "NormaliseGaussian": 0 },
        { "GP_conv2D": {
            "IH_IW_IC_OC": [13, 13, 1, 1],
            "kernel_size": 3, "stride": 2, "num_gp_pts": 10,"use_double_layer": true
        }},
        { "NormaliseGaussian": 0 },
        { "ReshapeGaussian": [-1, 36] },
        { "LayerFused": {
            "in": 36, "out": 10, "num_gp_pts": 10
        }},
        { "NormaliseGaussian": 0 },
        { "LayerFused": {
            "in": 10, "out": 10, "num_gp_pts": 10
        }}
    ]
